{
    "app_name":"crazybride",
    "is_platform":true,
    "frameworks":{
        "angular":"angular"
    },
    "models":{
        "collection":{
            "parent":"account",
            "uri_prefix":"/collection",
            "fields":{
                "name":"string",
                "namespace":"string",
                "desc":"md",
                "original_img":"s3-asset",
                "thumb_img":"s3-asset",
                "website_url":"string",
                "budget":"number",
                "guests":"number",
                "city":{
                    "type":"ref",
                    "ref":"city",
                    "bootstrap_populate":"req.user"
                },
                "account":{
                    "type":"ref",
                    "ref":"account",
                    "bootstrap_populate":"req.user"
                },
                "archiveDate":"date"
            }
        },
        "vendor":{
            "uri_prefix":"/vendors",
            "fields":{
                "name":"string",
                "namespace":"string",
                "desc":"md",
                "original_img":"s3-asset",
                "thumb_img":"s3-asset",
                "website_url":"string",
                "city":{
                    "type":"ref",
                    "ref":"city",
                    "bootstrap_populate":"req.user"
                },
                "account":{
                    "type":"ref",
                    "ref":"account",
                    "bootstrap_populate":"req.user"
                },
                "archiveDate":"date"
            }
        },
        "service":{
            "uri_prefix":"/services",
            "fields":{
                "name":"string",
                "namespace":"string",
                "desc":"md",
                "original_img":"s3-asset",
                "thumb_img":"s3-asset",
                "website_url":"string",
                "vendor":{
                    "type":"ref",
                    "ref":"vendor",
                    "bootstrap_populate":"req.vendor"
                },
                "archiveDate":"date"
            }
        }

    },
    "bowser":{
        "dependencies":{
            "font-awesome":"*",
            "angular":"*",
            "angular-route":"*",
            "angular-cookie":"*",
            "angular-resource":"*",
            "angular-strap":"*"
        }
    },
    "package":{
        "dependencies":{
            "mkdirp":"*",
            "hogan.js":"*",
            "aws-sdk":"*"
        }
    }
}